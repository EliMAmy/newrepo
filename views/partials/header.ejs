<header  id="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </span>
  <div id="tools">
    <% if (loggedin) { %>
      <!-- Only visible when logged in -->
      <a href="/account/management" title="Account Management">Welcome <%= accountData.account_firstname %></a>
      <a href="/account/logout" title="Click to log out">Logout</a>
    <% } else { %>
      <!-- Only visible when not logged in -->
      <a href="/account/login" title="Click to log in">My Account</a>
    <% } %>
  </div>
</header>