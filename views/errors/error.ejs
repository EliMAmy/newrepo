<%# Assigment 3, Basic Error Handling Activity%>
<% if (title) { %>
<h1 class="number_error"><%= title %></h1>
<% } else {
  res.redirect('/')
} %>

<div class="error_404">
  <%# messages() %>
  <p>
    <%- message %>
  </p>
  <img 
    src="/images/error/error.webp" 
    alt="Application error"
    class="error-image"
  />
</div>
